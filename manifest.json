{
  "manifest_version": 3,
  "name": "YouTube Subscription Redirect",
  "version": "1.0",
  "description": "Redirects YouTube Home/Shorts to Subscription Feed",
  "permissions": ["tabs"],
  "host_permissions": ["*://*.youtube.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
